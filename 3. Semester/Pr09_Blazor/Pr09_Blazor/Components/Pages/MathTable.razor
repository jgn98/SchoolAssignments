@page "/mathtable/{Number:int}"
@rendermode InteractiveServer

<h3>Math Table</h3>

<table class="table">
    <tbody>
        @for (int i = 1; i <= 10; i++)
        {
            <tr>
                <td>@Number</td>
                <td>x</td>
                <td>@i</td>
                <td>=</td>
                <td>@(Number* i)</td>
            </tr>
        }
    </tbody>
</table>

@code {
    [Parameter]
    public int Number { get; set; }
}