<Page
    x:Class="HurtigBiludlejning.NewCarWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:HurtigBiludlejning"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="NewCarWindow"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="White"
    mc:Ignorable="d">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />


        </Grid.RowDefinitions>

        <Button
            x:Name="Back"
            Grid.Row="0"
            Grid.Column="0"
            BorderBrush="Black"
            Content="Back"
            Click="BackBtn_Click"/>
        <Button
            x:Name="Confirm"
            Grid.Row="12"
            Grid.Column="6"
            BorderBrush="Black"
            Content="Confirm" />
        <Image
            Grid.Row="0"
            Grid.RowSpan="2"
            Grid.Column="5"
            Grid.ColumnSpan="2"
            Source="/Images/HurtigBiludlejningLogo.jpg" />
        <TextBox
            x:Name="Make"
            Grid.Row="3"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            BorderBrush="Black" />
        <TextBox
            x:Name="Model"
            Grid.Row="5"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            BorderBrush="Black" />
        <TextBox
            x:Name="Fuel"
            Grid.Row="7"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            BorderBrush="Black" />
        <TextBox
            x:Name="Mileage"
            Grid.Row="9"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            BorderBrush="Black" />
        <TextBox
            x:Name="Category"
            Grid.Row="11"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            BorderBrush="Black" />
        <Label
            Grid.Row="2"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            HorizontalAlignment="Left"
            VerticalAlignment="Bottom"
            Content="Make"
            FontWeight="Bold" />
        <Label
            Grid.Row="4"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            HorizontalAlignment="Left"
            VerticalAlignment="Bottom"
            Content="Model"
            FontWeight="Bold" />
        <Label
            Grid.Row="6"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            HorizontalAlignment="Left"
            VerticalAlignment="Bottom"
            Content="Fuel"
            FontWeight="Bold" />
        <Label
            Grid.Row="8"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            HorizontalAlignment="Left"
            VerticalAlignment="Bottom"
            Content="Mileage"
            FontWeight="Bold" />
        <Label
            Grid.Row="10"
            Grid.Column="2"
            Grid.ColumnSpan="3"
            HorizontalAlignment="Left"
            VerticalAlignment="Bottom"
            Content="Category"
            FontWeight="Bold" />





    </Grid>
</Page>
